{
  "env": {
    "browser": true,
    "es2021": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": ["plugin:@typescript-eslint/recommended", "next"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "indent": ["warn", 4],
    "semi": ["error", "always"]
  }
}
