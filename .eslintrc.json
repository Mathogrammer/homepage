{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "next"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": "off",
    "indent": ["error", 4]
  }
}
